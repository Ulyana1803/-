#include "stdafx.h"
#include <iostream>
#include <math.h>
using namespace std;
float F1(float x)
{
float result = 0;
if (x >= 0 )
{
result = (x*x)+(2*x)+1;
}
else
{
result = abs(x-9);
}
return result;
}
   int main()
   {
   int start = 0;
  cout « "Input start: ";
  cin » start;
  int stop = 0;
  cout « "Input";
  cin » stop;
  for (int i = start; i < stop; i++)
  {
  float f = F1(i);
  cout « "i = " « i « "; F1(i) = " « f « endl;
  }
  
system("pause");
